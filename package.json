{
    "name": "eslint-plugin-require-extensions",
    "version": "0.1.3",
    "author": "Solana Maintainers <maintainers@solana.foundation>",
    "repository": "https://github.com/solana-labs/eslint-plugin-require-extensions",
    "license": "Apache-2.0",
    "type": "module",
    "scripts": {
        "fmt": "prettier --write '{*,**/*}.{ts,tsx,js,jsx,json}'",
        "test": "eslint . --report-unused-disable-directives"
    },
    "engines": {
        "node": ">=18"
    },
    "main": "./index.js",
    "exports": {
        "require": "./index.js",
        "import": "./index.js"
    },
    "files": [
        "index.js",
        "LICENSE",
        "README.md"
    ],
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "eslint": "*"
    },
    "devDependencies": {
        "eslint": "^9.24.0",
        "prettier": "^3.5.3"
    }
}
